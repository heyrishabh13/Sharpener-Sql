const express = require("express");
const studentController = require("../controllers/studentController");

const router = express.Router();

router.post(
  "/addingStudentWithCard",
  studentController.addingValuesToStudentAndIdentityCardTable
);
router.post(
  "/addingStudentWithPost",
  studentController.addingValuesToStudentAndPostTable
);
router.post("/addStudent", studentController.addValuesToStudentTable);

module.exports = router;
